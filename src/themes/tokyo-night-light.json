{
  "version": "1.0",
  "id": "tokyo-night-light",
  "name": "Tokyo Night Light",
  "description": "A clean light theme inspired by Tokyo daylight",
  "author": {
    "name": "enkia",
    "url": "https://github.com/enkia/tokyo-night-vscode-theme"
  },
  "variant": "light",
  "homepage": "https://github.com/enkia/tokyo-night-vscode-theme",
  "colors": {
    "background": {
      "primary": "#d5d6db",
      "secondary": "#cbccd1",
      "tertiary": "#b4b5b9"
    },
    "text": {
      "primary": "#343b58",
      "secondary": "#4c5178",
      "tertiary": "#9699a3"
    },
    "accent": {
      "primary": "#2e7de9",
      "subtle": "#cbccd1"
    },
    "border": {
      "default": "#b4b5b9",
      "subtle": "#cbccd1",
      "focus": "#2e7de9"

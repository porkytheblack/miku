      "md": "0 4px 12px rgba(52, 59, 88, 0.12)",
      "lg": "0 8px 24px rgba(52, 59, 88, 0.16)"
    },
    "highlights": {
      "clarity": "rgba(142, 114, 32, 0.25)",
      "grammar": "rgba(244, 93, 107, 0.2)",
      "style": "rgba(46, 125, 233, 0.2)",
      "structure": "rgba(150, 120, 221, 0.2)",
      "economy": "rgba(72, 131, 72, 0.2)"
    },
    "syntax": {
      "heading": "#2e7de9",
      "emphasis": "#4c5178",
      "codeBackground": "#b4b5b9",
      "codeText": "#f52a65",
